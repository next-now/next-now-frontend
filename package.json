{
  "name": "gatsby-starter-amazee",
  "private": true,
  "description": "Gatsby starter for Amazee Labs projects",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "mocks": "nodemon -w mocks -e ts ./node_modules/.bin/concurrently npm:mock:*",
    "mock:api": "ts-node mocks/swapi.ts",
    "test": "mocha -r ts-node/register src/**/*.spec.ts mocks/**/*.spec.ts",
    "tailwind": "tailwind build src/styles/tailwind.css > src/styles/tailwind.build.css",
    "cypress:open": "cypress open",
    "cypress:open:api": "CYPRESS_API_URL=http://localhost:4001 CYPRESS_TEST_LEVEL=api CYPRESS_TAGS='not @UI' cypress open",
    "cypress:run": "./cypress-tests",
    "cypress:run:api": "CYPRESS_API_URL=http://localhost:4001 ./cypress-tests api",
    "storybook": "NODE_ENV=production start-storybook -p 6006 --ci -s public",
    "storybook:build": "NODE_ENV=production build-storybook -s public"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.8",
    "apollo-server": "^2.9.13",
    "apollo-utilities": "^1.3.3",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.18.4",
    "gatsby-plugin-layout": "^1.1.18",
    "gatsby-plugin-postcss": "^2.1.16",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-typescript": "^2.1.20",
    "gatsby-source-graphql": "2.1.25",
    "graphql": "^14.6.0",
    "i18next": "^19.0.2",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.2.6",
    "start-server-and-test": "^1.10.6",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@cypress/webpack-preprocessor": "latest",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/core": "^5.3.17",
    "@storybook/preset-typescript": "^1.2.2",
    "@storybook/react": "^5.3.17",
    "@storybook/source-loader": "^5.3.17",
    "@testing-library/cypress": "^5.0.2",
    "@types/chai": "^4.2.10",
    "@types/classnames": "^2.2.9",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "chai": "^4.2.0",
    "cypress": "^3.7.0",
    "cypress-cucumber-preprocessor": "https://github.com/liamchilds/cypress-cucumber-preprocessor.git#configurable-loc-non-global-steps",
    "cypress-wait-until": "^1.6.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-is": "^16.12.0",
    "regenerator-runtime": "^0.13.5",
    "tailwindcss": "^1.2.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.6"
  },
  "resolutions": {
    "graphql-tools": "^4.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/gatsby-starter-amazee"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/gatsby-starter-amazee/issues"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run format:check || echo -e '\\n\\033[0;31mWARNING:  YOUR PR WILL HAVE FAILING TESTS.\\033[0m\\n'"
    }
  }
}
